{
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "status": 200,
      "src": "/db",
      "dest": "/links.js",
      "headers": { "cache-control": "maxage=36000, s-maxage=18000, stale-while-revalidate" }
    },
    {
      "src": "/(.*)",
      "dest": "server.js",
      "continue": true,
      "headers": { "cache-control": "s-maxage=36000, stale-while-revalidate" }
    }

  ]
}
